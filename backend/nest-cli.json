{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "elasticsearch": {
      "type": "library",
      "root": "libs/elasticsearch",
      "entryFile": "index",
      "sourceRoot": "libs/elasticsearch/src",
      "compilerOptions": {
        "tsConfigPath": "libs/elasticsearch/tsconfig.lib.json"
      }
    },
    "weaviate": {
      "type": "library",
      "root": "libs/weaviate",
      "entryFile": "index",
      "sourceRoot": "libs/weaviate/src",
      "compilerOptions": {
        "tsConfigPath": "libs/weaviate/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "postgres": {
      "type": "library",
      "root": "libs/postgres",
      "entryFile": "index",
      "sourceRoot": "libs/postgres/src",
      "compilerOptions": {
        "tsConfigPath": "libs/postgres/tsconfig.lib.json"
      }
    },
    "rabbit": {
      "type": "library",
      "root": "libs/rabbit",
      "entryFile": "index",
      "sourceRoot": "libs/rabbit/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rabbit/tsconfig.lib.json"
      }
    }
  }
}